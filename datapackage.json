{
  "datapackage_version": "1.0-beta.7",
  "name": "dataset-mod-disposals",
  "title": "Disposal database: House of Commons report",
  "description": "MoD present and future disposal properties that are in the public domain that is provided for reference in the House of Commons library. This version is being used for GitHub workflow experimentation by the ODI.",
  "licenses": [
    {
      "id": "ogl",
      "url": "http://reference.data.gov.uk/id/open-government-licence"
    }
  ],
  "publisher": [
    {
      "name": "Defence Infrastructure Organisation",
      "web": "http://data.gov.uk/publisher/defence-infrastructure-organisation"
    }
  ],
  "keywords": [
    "Defence",
    "Government",
    "Land and Property",
    "Property",
    "disposals",
    "house of commons"
  ],
  "sources" : [ 
    {
      "name": "data.gov.uk",
      "web": "http://data.gov.uk/dataset/defence-infrastructure-organisation-disposals-database-house-of-commons-report"
    },
    {
      "name": "gov.uk",
      "web": "https://www.gov.uk/government/publications/disposal-database-house-of-commons-report"
    }
  ],
  "resources": [
    {
      "url": "https://raw.github.com/theodi/dataset-mod-disposals/master/disposals.csv", 
      "path": "disposals.csv",
      "mediatype": "text/csv",
      "schema": {
        "fields": [
          {
            "name": "MOD CRN",
            "type": "string"
          },
          {
            "name": "Name",
            "type": "string"
          }, 
          {
            "name": "Address",
            "type": "string"
          }, 
          {
            "name": "Town",
            "type": "string"
          }, 
          {
            "name": "County",
            "type": "string"
          }, 
          {
            "name": "Area (Ha)",
            "label": "Area",
            "type": "string",
            "unit": {
              "id": "Ha",
              "type": "physical"
            }
          }, 
          {
            "name": "Grid Reference",
            "type": "string"
          }, 
          {
            "name": "Property Type",
            "type": "string"
          }, 
          {
            "name": "Expected Planning Use",
            "type": "string"
          }, 
          {
            "name": "Expected Number of Housing Units",
            "type": "string"
          }, 
          {
            "name": "Constituency",
            "type": "string"
          }, 
          {
            "name": "Financial Year",
            "type": "date"
          }
        ]
      }
    }
  ]
}
